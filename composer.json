{
  "name": "clsimplex/helpers",
  "description": "Collection of simple helper functions for our Laravel based meta-platform, tuxedo.",
  "keywords": ["laravel", "clsimplex", "helpers", "tuxedo"],
  "repositories": [],
  "support": {
    "email": "info@clsimplex.com"
  },
  "license": "MIT",
  "homepage": "https://clsimplex.com",
  "authors": [
    {
      "name": "Levon Zadravec-Powell",
      "homepage": "https://clsimplex.com",
      "email": "levon@clsimplex.com"
    },
    {
      "name": "Charles Moolin",
      "homepage": "https://clsimplex.com",
      "email": "charles@clsimplex.com"
    }
  ],
  "prefer-stable": true,
  "minimum-stability": "stable",
  "require": {
    "nesbot/carbon": "~1.20"
  },
  "require-dev": {
    "php": ">=5.6.4",
    "illuminate/support": "~5",
    "phpmetrics/phpmetrics": "~2.4.1",
    "phpmd/phpmd": "~2.6.0",
    "phpunit/phpunit": "~7.3",
    "friendsofphp/php-cs-fixer": "~2.13.0",
    "squizlabs/php_codesniffer": "3.*",
    "vimeo/psalm": "2.0.11",
    "phpstan/phpstan": "0.10.3",
    "phpstan/phpstan-phpunit": "0.10"
  },
  "autoload": {
    "psr-4": {
      "CLSimplex\\Tuxedo\\Helpers\\": "src/Classes/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CLSimplex\\Tuxedo\\Helpers\\": "src/Classes/",
      "CLSimplex\\Tuxedo\\Helpers\\tests\\": "/tests"
    }
  }
}
